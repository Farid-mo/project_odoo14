<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>

    <record model="ir.ui.view" id="enterprise_job_tree_view">
      <field name="name">Enterprise job list</field>
      <field name="model">enterprise.job</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
<!--          <field name="salary"/>-->
<!--          <field name="email"/>-->
<!--          <field name="content"/>-->
          <field name="employee_uid"/>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="enterprise_job_form_view">
      <field name="name">Enterprise job form_list</field>
      <field name="model">enterprise.job</field>
      <field name="arch" type="xml">
        <form>
          <group>
              <field name="name"/>
<!--            <field name="salary"/>-->
<!--            <field name="email"/>-->
<!--            <field name="content"/>-->
            <field name="employee_uid"/>
          </group>
        </form>
      </field>
    </record>


  </data>
</odoo>
